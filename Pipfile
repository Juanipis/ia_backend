[[source]]
url = "https://pypi.org/simple"
verify_ssl = true
name = "pypi"

[packages]
fastapi = "*"
uvicorn = {extras = ["standard"], version = "*"}

[dev-packages]

[requires]
python_version = "3.12"

[scripts]
start_dev = "uvicorn app.main:app --reload"
test = "python -m unittest discover -s . -p '*_test.py'"
start_prod = "uvicorn app.main:app --host 0.0.0.0 --port 80"

